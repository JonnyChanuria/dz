package com.company;
import java.util.ArrayList;
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        int n = 3;
        ArrayList<String> first = new ArrayList<String>(n);
        first.add("мама");
        first.add("мыла");
        first.add("раму");
        for(int i = 0; i < 6; i = i+2){
            first.add(i,"именно");
        }
        for(int i = 0; i < 6; i++){
            System.out.println(first.get(i));
        }
    }
}
